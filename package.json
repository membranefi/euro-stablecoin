{
  "scripts": {
    "generate:typechain": "npm run generate:oz-typechain && npm run generate:own-typechain",
    "generate:oz-typechain": "npx typechain --target ethers-v5 --out-dir typechain/openzeppelin \"./artifacts/@openzeppelin/**/+([a-zA-Z0-9_]).json\"",
    "generate:own-typechain": "npx typechain --target ethers-v5 --out-dir typechain/euro \"./artifacts/contracts/**/+([a-zA-Z0-9_]).json\"",
    "run:slither": "slither . --filter-paths \"openzeppelin\" --print human-summary",
    "run:coverage": "set TS_NODE_TRANSPILE_ONLY=1 && npx hardhat coverage",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@openzeppelin/contracts": "4.7.2",
    "@openzeppelin/contracts-upgradeable": "4.7.1"
  },
  "devDependencies": {
    "@ethersproject/hardware-wallets": "^5.6.1",
    "@gnosis.pm/safe-ethers-adapters": "^0.1.0-alpha.9",
    "@gnosis.pm/safe-ethers-lib": "^1.1.0",
    "@gnosis.pm/safe-service-client": "^1.1.2",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/hardhat-upgrades": "^1.17.0",
    "@typechain/ethers-v5": "^10.0.0",
    "@typechain/hardhat": "^6.1.0",
    "@types/chai": "^4.3.1",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.35",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^16.0.1",
    "eth-permit": "^0.2.1",
    "eth-typed-data": "^0.1.0-beta.0",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.6.8",
    "hardhat": "^2.9.9",
    "hardhat-deploy": "^0.11.10",
    "hardhat-safe-deployer": "github://rmeissner/hardhat-safe-deployer",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "solidity-coverage": "^0.7.21",
    "ts-node": "^10.8.0",
    "typechain": "^8.0.0",
    "typescript": "^4.7.2"
  }
}
